<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="fence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>hedge singleton</name>
        <functions>
            <function>$l10n_function_decoration</function>
        </functions>
        <image>maps/placeables/store/storeHedge.dds</image>
        <price>5</price>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>fences</category>
        <canBeSold>true</canBeSold>
        <showInStore>false</showInStore>
        <brush>
            <type>none</type>
        </brush>
    </storeData>

    <base>
        <filename>maps/placeables/hedge/hedge.i3d</filename>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
        <sounds>
            <place template="foliageBig" volumeScale="0.4" pitchScale="1.4"/>
            <destroy template="destroyFence" />
        </sounds>
    </placement>

    <fence maxVerticalAngle="50" maxVerticalGateAngle="2" hasInvisiblePoles="true">
        <poles node="0>" />
        <panels node="1>" length="8" /> <!-- contains list of children each is a variant -->
    </fence>

    <clearAreas/>
    <leveling/>
    <indoorAreas/>
    <ai><updateAreas/></ai>
</placeable>

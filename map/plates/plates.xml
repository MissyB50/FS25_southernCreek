<licensePlates xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/mapLicensePlates.xsd">
    <font name="LICENSE_PLATE_UK" />


    <licensePlate filename="map/plates/licensePlates.i3d" node="0" type="ELONGATED" width="0.52" height="0.11">
        <font size="0.065" scaleX="0.75" scaleY="1"/>

        <variations>
            <variation> <!-- 1 region, 2 letter, 3 numbers -->
                <value node="0|0" posX="-0.172" numerical="false" alphabetical="true" locked="false"/>
                <value node="0|1" posX="-0.109" numerical="false" alphabetical="true" />
                <value node="0|2" posX="-0.048" numerical="true" alphabetical="false" />
                <value node="0|3" posX="0.015" numerical="true" alphabetical="false" />
				
                <value node="0|4" posX="0.089"  numerical="false"  alphabetical="true" nextSection="true" />
                <value node="0|5" posX="0.153"  numerical="false"  alphabetical="true" />
                <value node="0|6" posX="0.215"  numerical="false"  alphabetical="true" />

                <color shaderParameter="colorMat0" color="0 0.033 0.319"/>
            </variation>
            <variation> <!-- fantasy license plate -->
                <value node="0|0" posX="-0.172" numerical="false" alphabetical="true" special="true"/>
                <value node="0|1" posX="-0.109" numerical="false" alphabetical="true" special="true"/>
                <value node="0|2" posX="-0.048" numerical="true" alphabetical="false" special="true"/>
                <value node="0|3" posX="0.015" numerical="true" alphabetical="false" special="true"/>
                <value node="0|4" posX="0.089"  numerical="false" alphabetical="true" special="true"/>
                <value node="0|5" posX="0.153"  numerical="false" alphabetical="true" special="true"/>
                <value node="0|6" posX="0.215"  numerical="false" alphabetical="true" special="true"/>

                <color shaderParameter="colorMat0" color="0 0.033 0.319"/>
            </variation>
        </variations>
    </licensePlate>


